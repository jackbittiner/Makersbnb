<html>
  <body>

    <button action="/spaces/new">
      List a Space
    </button>

    <div>
      <ul id="spaces">
        <% @spaces.each do |space| %>
          <%= space.name %>
          <%= space.description %>
          <%= space.price %>
          <%= space.available_from %>
          <%= space.available_to %>
        <% end %>
      </ul>
    </div>
  </body>
</html>
